<style type="text/css">
    .root {
        display: flex;
        background-color: #ecf0f1;
        justify-content: center;
        align-items: center;


        flex-direction: column;
    }

    .subdiv {
        width: 800px;

        background-color: #FFF;
        padding: 25px;
        border-radius: 20px;
        margin-bottom: 20px;

    }
</style>


<html>

<body style=" background-color: #ecf0f1;">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <div class="root">
        <div style="padding: 10px; font-size: 20px;font-weight: bold;letter-spacing: 2px;">
            <img src="/images/supplier.png" width="40" />
            List of User_Detail
        </div>
        <div class="subdiv">
            <a href="/adduser/userdetail">Add New User</a>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">ID</th>
                        <th scope="col">Name</th>
                        <th scope="col">MobileNO</th>
                        <th scope="col">EmaildId</th>
                    </tr>
                </thead>
                <tbody>
                    <% result.map((item,index)=>{ %>

                        <tr>
                            <th scope="row">
                                <%=(index+1) %>
                            </th>


                            <td>
                                <div>
                                    <%=item.username %>
                                </div>
                            </td>


                            <td>
                                <div>
                                    <%=item.mobileno %>
                                </div>
                            </td>


                            <td>
                                <div>
                                    <%=item.emailid %>
                                </div>
                            </td>

                        </tr>
                        <% }) %>
                </tbody>
            </table>

        </div>


    </div>
</body>

</html>